<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <mapped-superclass name="Nicolassing\SequenceBundle\Model\Sequence">

        <field name="index" column="seq_index" type="integer" />

        <field name="type" column="seq_type" />

        <field name="prefix" column="seq_prefix" nullable="true" />

        <field name="version" column="seq_version" type="integer" version="true" />

    </mapped-superclass>

</doctrine-mapping>